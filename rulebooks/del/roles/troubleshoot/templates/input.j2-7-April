{
    "link":[
{% for path in path_data %}
        {
              "start_task_result":{{ path[1] }},
              "possibility":{{ path[3] }},
              "start_task":"{{ path[0]|upper }}",
              "next_task":"{{ path[2]|upper }}",
              "record":"{{ path[0]|upper }}-{{ path[1] }}-{{ path[2]|upper }}:{{ path[3] }}",
        },
{% endfor %}
    ],
    "queryPath": [
        {
              "start":"{{ start|upper }}",
              "end":"{{ end|upper }}"
        }
    ]
}

