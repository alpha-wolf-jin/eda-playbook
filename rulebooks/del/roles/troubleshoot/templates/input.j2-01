{
    "path":[
{% for path in path_data %}
        {
              "start_task_result":{{ path[2] }},
              "start_task":"{{ path[0]|lower }}",
              "next_task":"{{ path[1]|lower }}",
              "record":"<{{ path[0]|lower }},{{ path[2] }},{{ path[1]|lower }}>",
        },
{% endfor %}
    ],
    "task": [
        {
              "task_result":"{{ start_result }}",
              "task":"{{ start|lower }}"
        }
    ]
}

